    prefix: "tag"
    body: ""
  "":    # fa-calendar \uF073
    prefix: "date"
    body: ""
  "":    # fa-linux \uF17C
    prefix: "linux"
    body: ""
  "":    # fa-windows \uF17A
    prefix: "windows"
    body: ""
