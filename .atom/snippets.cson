"_note": "to apply snippet require reloading atom or press ctrl-alt-r"

".source":
  "BUG":
    prefix: "bug"
    body: "BUG"
  "NOTE":
    prefix: "note"
    body: "NOTE"
  "TODO":
    prefix: "todo"
    body: "TODO"

".source.js":
  "(() => {...})()":
    prefix: "iife"
    body: """
      (() => {

      })()"""
  "map(fcn(...))":
    prefix: "mapf"
    body: """
      map(function($1){
        $2
        return ;
      });"""

  "sort(fcn(...))":
    prefix: "sortf"
    body: """
      sort(function(${1:a}, ${2:b}){return $3;});"""

  "it('spec', ()=>{})":
    prefix: "it"
    body: """
      it('${1:spec description}', (done) => {
        done();
        $2
      });"""

  "import... export...":
    prefix: "init"
    body: """
      import React from 'react'

      const ${1:myComponent} = () => {
        return (
          <${2:div}></${2:div}>
        )
      }

      export default ${1:myComponent}"""

  "console.log({ foo })":
    prefix: "log(...)"
    body: "console.log({ ${1:foo} })"

  "console.warn({ foo })":
    prefix: "warn(...)"
    body: "console.warn({ ${1:foo} })"

  "console.info({ foo })":
    prefix: "info(...)"
    body: "console.info({ ${1:foo} })"

  "/*: Object */":
    prefix: "flow comment syntax"
    body: "/*: ${1:Object} */"

  "#!/usr/bin/env node":
    prefix: "shebang"
    body: "#!/usr/bin/env node"

".source.coffee":
  "it('msg', ()->{})":
    prefix: "it"
    body: """
      it "${1:spec description}", ->
        ${2:foo()}"""

".source.java":
  "System.out.println()":
    prefix: "sout"
    body: "System.out.println(${1:\"\"});$2"

".source.php":
  ".'<br>'":
    prefix: "br"
    body: ".'<br>'"

".source.python":
  "__init__(self)":
    prefix: "init"
    body: """
      def __init__(self, ${1:arg}):
          self.${1:arg} ="""

  "if __name__ == '__main__'":
    prefix: "ifmain"
    body: """
      if __name__ == '__main__':
          ${1:foo()}"""

".source.shell":
  "for x in ...; do ... done":
    prefix: "forin"
    body: """
      for ${1:x} in ${2:1 2 3}; do
        echo $${1:x}
      done"""

  "declare -a arr=(...); for x in \"${arr[@]}\"; do ...":
    prefix: "forinarray"
    body: """
      declare -a ${1:arr}=(
        "item1" \\
        "item2" \\
      )
      for ${2:x} in "$\{${1:arr}[@]\}"; do
        echo $${2:x}
      done"""

  "#!/bin/sh ... set -e":
    prefix: "shebang"
    body: """
      #!/bin/sh

      set -e"""

".source.gfm":
